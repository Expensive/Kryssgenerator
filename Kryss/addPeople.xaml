        <Label Content="Efternamn:" Height="27" HorizontalAlignment="Left" Margin="11,104,0,0" Name="lblEfternamn" VerticalAlignment="Top" Width="74" />
        <Button Content="Lägg till" Height="64" HorizontalAlignment="Left" Margin="50,194,0,0" Name="addP_Btn" VerticalAlignment="Top" Width="99" Click="addP_Btn_Click" />
